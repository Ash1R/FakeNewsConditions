        "import re\n",
        "\n",
        "def clean_text(text):\n",
        "    index = text.name\n",
        "    text = list(text)[0]\n",
        "\n",
        "    text = re.sub(r'\\([^]]*\\)', '', text) \n",
        "    text = re.sub(r'\\[[^]]*\\]', '', text) \n",
        "    text = re.sub(r'([\\.\\\\\\+\\*\\?\\[\\^\\]\\$\\(\\)\\{\\}\\!\\<\\>\\|\\:\\-\\,\\\"\\”\\“\\‘])', '', text) \n",
        "    text = remove_stopwords(text)       \n",
        "    return \" \".join(text)\n",
        "\n",
        "def remove_stopwords(text):\n",
        "    new_text = []\n",
        "    for i in text.lower().split():\n",
        "        word = i.strip()\n",
        "        if word not in stop_words:\n",
        "            new_text.append(i)\n",
        "    return new_text\n",
        "\n",
        "\n",
        "\n",
        "df['text'] = (df['title'] + df['text']).to_frame(0).apply(clean_text, axis = 1)\n",
        "df"
      ],
